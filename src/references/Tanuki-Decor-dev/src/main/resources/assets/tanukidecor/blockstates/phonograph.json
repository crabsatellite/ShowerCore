{
  "multipart": [
    {
      "when": { "half": "upper", "facing": "north" },
      "apply": { "model": "tanukidecor:block/phonograph/upper" }
    },
    {
      "when": { "half": "lower", "facing": "north" },
      "apply": { "model": "tanukidecor:block/phonograph/lower" }
    },
    {
      "when": { "half": "upper", "facing": "south" },
      "apply": { "model": "tanukidecor:block/phonograph/upper", "y": 180 }
    },
    {
      "when": { "half": "lower", "facing": "south" },
      "apply": { "model": "tanukidecor:block/phonograph/lower", "y": 180 }
    },
    {
      "when": { "half": "upper", "facing": "east" },
      "apply": { "model": "tanukidecor:block/phonograph/upper", "y": 90 }
    },
    {
      "when": { "half": "lower", "facing": "east" },
      "apply": { "model": "tanukidecor:block/phonograph/lower", "y": 90 }
    },
    {
      "when": { "half": "upper", "facing": "west" },
      "apply": { "model": "tanukidecor:block/phonograph/upper", "y": 270 }
    },
    {
      "when": { "half": "lower", "facing": "west" },
      "apply": { "model": "tanukidecor:block/phonograph/lower", "y": 270 }
    }
  ]
}
